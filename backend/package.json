{
  "name": "cpass-server",
  "main": "index.ts",
  "packageManager": "yarn@3.2.0",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "lint": "yarn run eslint .eslintrc.js \"./**/*.{ts,js}\"",
    "build": "tsc --build --clean && tsc --build",
    "start-build": "node ./build/index.js",
    "lint-and-start": "concurrently \"yarn run lint\" \"yarn run start-build\"",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"yarn run lint-and-start\"",
    "start": "yarn build && yarn start-build"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "colors": "1.4.0",
    "cors": "2.8.5",
    "date-fns": "2.28.0",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "google-auth-library": "7.14.1",
    "googleapis": "99.0.0",
    "joi": "17.6.0",
    "mongodb": "4.4.1",
    "morgan": "1.10.0",
    "nodemailer": "6.7.3",
    "nodemon": "2.0.15",
    "object-hash": "3.0.0",
    "qrcode": "1.5.0",
    "speakeasy": "2.0.0",
    "ts-node": "10.7.0",
    "tsc-watch": "4.6.2",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "@types/nodemailer": "6.4.4",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "concurrently": "7.0.0",
    "eslint": "8.12.0",
    "typescript": "4.6.3"
  }
}